{
  "name": "regtank-client-portal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "build:test": "yarn unlink @protego/sdk;yarn run reinstall-sdk;yarn build;serve -s build",
    "extract": "formatjs extract",
    "git:pr": "gh pr create --base develop-review-code --reviewer theoislandpeak",
    "lint": "prettier --write \"src/**/*.js\"",
    "reinstall-sdk": "yarn add git+ssh://git@github.com/Regtank-Technology-Pte-Ltd/protego-client-portal-frontend-sdk.git",
    "start": "node scripts/start.js",
    "start:withLocalSdk": "yarn link @protego/sdk;yarn start",
    "switch-sdk:local": "yarn unlink @protego/sdk;yarn run reinstall-sdk; yarn link @protego/sdk",
    "test": "node scripts/test.js",
    "typings:api": "protego tsch /v3/api-docs src/types/typings-api.d.ts --jsonPath components.schemas",
    "update:sdkVer": "cd ../../protego-fe-sdk;yarn bump:patch;cd ../ProtegoClientPortal/Frontend;yarn reinstall-sdk"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^react-native$": "react-native-web"
    },
    "modulePaths": [
      "/Users/macbook/WebstormProjects/ProtegoClientPortal/Frontend/src"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
